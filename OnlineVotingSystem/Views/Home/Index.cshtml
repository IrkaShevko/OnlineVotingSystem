@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="navbar-text">Number of active votings: ###</div>
    <div class="clearfix"></div>
<div class="col-xs-7">
    <table id="tables" class="table table-bordered table-hover">
        <thead>
            <tr class="head-table">
                <th class="text-center">Voting Name<i class="fa fa-sort icons-margin" data-bind="click: sortTableBy.bind($data, 'name')"></i></th>
                <th class="text-center">User who Add<i class="fa fa-sort icons-margin" data-bind="click: sortTableBy.bind($data, 'user')"></i></th>
                <th class="text-center">Voices Count<i class="fa fa-sort icons-margin" data-bind="click: sortTableBy.bind($data, 'voices')"></i></th>
                <th class="text-center">Comments<i class="fa fa-sort icons-margin" data-bind="click: sortTableBy.bind($data, 'comments')"></i></th>
            </tr>
        </thead>
        <tbody class="text-center" data-bind="foreach: votings">
            <tr>
                <td><a href="/Vote/VotingDetails" data-bind="text: $data.name"></a></td>
                <td data-bind="text: $data.userName"></td>
                <td data-bind="text: $data.voicesCount"></td>
                <td data-bind="text: $data.commentsCount"></td>
            </tr>
        </tbody>
    </table>
    <div align="center">
        <ul class="pagination">
            <li><a href="#"><<</a></li>
            <li><a href="#">1</a></li>
            <li><a href="#">2</a></li>
            <li><a href="#">>></a></li>
        </ul>
    </div>
</div>
<div class="col-xs-5"></div>
@section Scripts
{
    <script src="/Scripts/custom/controllers/homePageController.js"></script>
}



